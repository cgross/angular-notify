{
  "name": "angular-notify",
  "version": "2.5.0",
  "description": "A minimalistic notification service for angular.",
  "repository": {
    "type": "git",
    "url": "git://github.com/cgross/angular-notify.git"
  },
  "scripts": {
    "pretest": "npm install && bower install",
    "test": "node_modules/karma/bin/karma start karma.conf.js --no-auto-watch --single-run --reporters dots",
